{
  "name": "academe-flow-backend",
  "version": "1.0.0",
  "description": "academe flow web app backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm run start --prefix frontend",
    "client-prod": "npm run start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "concurrently \"npm run server\" \"npm run client-prod\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "author": "Fillip Kairos Lazarte",
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^5.10.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "next": "^14.2.5",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.7.5",
    "nodemon": "^3.1.4",
    "require-at": "^1.0.6",
    "socket.io": "^4.7.4",
    "tailwindcss": "^3.1.8",
    "toidentifier": "^1.0.1",
    "typescript": "^4.7.4",
    "undefsafe": "^2.0.5",
    "update-notifier": "^7.2.0",
    "xoauth2": "^1.2.0"
  }
}
